    <Action Name="A_CallFB"
    Id="{dd2aeea0-648d-4418-8282-2b766f935b43}">
      <Implementation>
        <ST>
          <![CDATA[(* FB Call *)
fbSFC_SamplePOU(
        eOperation      := eOperation,
        iResultC        => iResultC
);

]]>
</ST>
      </Implementation>
    </Action>
    <Action Name="A_Evaluate"
    Id="{ef2d306b-ff73-431c-816c-1ba3a6a01d15}">
      <Implementation>
        <ST>
          <![CDATA[(* Evaluation *)
IF fbSFC_SamplePOU.iResultC = 0 THEN
        bZero := TRUE;
        bPos := FALSE;
        bNeg := FALSE;
ELSIF fbSFC_SamplePOU.iResultC > 0 THEN
        bZero := FALSE;
        bPos := TRUE;
        bNeg := FALSE;
ELSIF fbSFC_SamplePOU.iResultC < 0 THEN
        bZero := FALSE;
        bPos := FALSE;
        bNeg := TRUE;
END_IF]]>
</ST>
      </Implementation>
    </Action>
    <Action Name="A_StateMachine"
    Id="{affee7ba-3d44-4d14-91a6-eaeb84c1f26d}">
      <Implementation>
        <ST>
          <![CDATA[(* State machine *)
CASE eOperation OF
eOp_Add:
        eOperation := eOp_MUL;
eOp_Sub:
        eOperation := eOp_ADD;
eOp_Mul:
        eOperation := eOp_SUB;
END_CASE]]>
</ST>
      </Implementation>
    </Action>
    <Action Name="A_Test"
    Id="{6b3d35f2-6009-43a9-8a7d-0507dca52086}">
      <Implementation>
        <ST>
          <![CDATA[bTest := (((bZero AND bPos) OR (bPos XOR bNeg)) AND (Timer.ET > TIME#500MS) AND (iResultC <= 0)) OR (Timer.ET <= TIME#500MS);
]]>
</ST>
      </Implementation>
    </Action>
    <Action Name="A_Wait"
    Id="{6ea0d5e9-8aed-4e70-8927-c9bdc537f79a}">
      <Implementation>
        <ST>
          <![CDATA[(* Timer *)
Timer(IN := TRUE, PT := tTimerValue);]]>
</ST>
      </Implementation>
    </Action>
    <Action Name="Init_Action"
    Id="{9290868f-4c38-47e6-85f1-af5806d0d355}">
      <Implementation>
        <ST>
          <![CDATA[Timer(IN := FALSE);]]>
</ST>
      </Implementation>
    </Action>
